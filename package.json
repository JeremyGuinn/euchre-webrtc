{
  "name": "euchre-webrtc",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:gh-pages": "PUBLIC_URL=/euchre-webrtc/ react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --cache",
    "format": "prettier --cache --write . ",
    "format:check": "prettier --cache --check .",
    "validate": "pnpm lint && pnpm typecheck && pnpm format:check",
    "prepare": "lefthook install && pnpm validate"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "clsx": "^2.1.1",
    "isbot": "^5.1.27",
    "peerjs": "^1.5.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "lefthook": "^1.9.3",
    "lint-staged": "^15.3.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --cache --write"
    ],
    "*.{json,md,css}": [
      "prettier --cache --write"
    ]
  },
  "packageManager": "pnpm@10.6.2+sha256.20072a1f6edd17646ea9234bf32c42d563dad37b2973e97a2dde5c17774a824d",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild"
    ]
  }
}
